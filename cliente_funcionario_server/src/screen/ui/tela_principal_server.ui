QMainWindow {
    background-color: #f9f9f9; 
    border: 1px solid #ccc
}

QTabWidget::pane {
    border: 1px solid #aaa; 
    background-color: #ffffff;
}

QTabBar::tab {
    background: #e0e0e0;
    padding: 5px 10px;
    border: 1px solid #aaa;
    border-bottom: none;
    color: #333;
}

QTabBar::tab:selected {
    background: #ffffff;
    border-bottom: 1px solid #fff;
}

QListView {
    background: #ffffff;
    border: 1px solid #ccc;
    color: #333;
    padding: 5px;
}

QPushButton {
    background-color: #0078d4;
    color: white;
    border: 1px solid #005a9e;
    border-radius: 5px;
    padding: 5px 10px;
}

QPushButton:hover {
    background-color: #005a9e;
}

QPushButton:pressed {
    background-color: #003f7e;
}

QComboBox {
    background-color: #ffffff;
    border: 1px solid #ccc;
    border-radius: 5px;
    padding: 5px;
    color: #333;
}

QComboBox:disabled {
    background-color: #f0f0f0; /* Cor de fundo para desativado */
    border: 1px solid #aaa; /* Cor da borda quando desativado */
    color: #888; /* Cor do texto quando desativado */
}

QComboBox::drop-down {
    border-left: 1px solid #aaa;
    width: 20px;
    background: #e0e0e0;
}

QMenuBar {
    background-color: #e0e0e0;
    border-bottom: 1px solid #ccc;
}

QMenuBar::item {
    padding: 5px 10px;
    background: transparent;
}

QMenuBar::item:selected {
    background: #d0d0d0;
}

QMenu {
    background: #ffffff;
    border: 1px solid #ccc;
}

QMenu::item:selected {
    background: #0078d4;
    color: white;
}

/* Separadores visuais */
QListView::item {
    border-bottom: 1px solid #ddd; /* Linha entre os itens */
    padding: 2px;
    padding-left: 12px; /* Espaço reservado para o indicador */
    position: relative; /* Necessário para posicionar o indicador */
}

/* Indicador de status */
QListView::item {
    content: &quot;&quot;;
    position: absolute;
    left: 0; /* Posicionado no início do item */
    top: 0;
    bottom: 0;
    width: 16px; /* Largura do indicador */
}

/* Itens selecionados */
QListView::item:selected {
    background-color: #0078d4;
    color: white;
}

/* Separadores especiais */
QListView::item[separator=&quot;true&quot;] {
    background-color: #f0f0f0;
    font-weight: bold;
    border-bottom: 2px solid #aaa;
}
